@model IEnumerable<KreditDateVM>
@{
    ViewData["Title"] = "KreditDateReport";
}

<h2>Kreditek</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                Vezetéknév
            </th>
            <th>
                Keresztnév
            </th>
            <th>
                Kreditek száma
            </th>
            <th>
                Kredit stringben
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.LastName
                </td>
                <td>
                    @item.FirstMidName
                </td>
                <td>
                    @item.Gradesint
                </td>
            </tr>
        }
    </tbody>
</table>

